{
  "private": true,
  "name": "cgmd-prevue",
  "version": "0.0.0",
  "description": "realtime markdown preview for cgmd-preview",
  "scripts": {
    "dev": "node tasks/runner.js",
    "lint": "eslint --ext .js,.vue -f ./node_modules/eslint-friendly-formatter app",
    "lint:fix": "eslint --ext .js,.vue -f ./node_modules/eslint-friendly-formatter --fix app",
    "pack": "cross-env NODE_ENV=production webpack -p --progress --colors",
    "postinstall": "cd app && npm install",
    "vuex:module": "node tasks/vuex/module.js",
    "build": "rm -rf builds/cgmd-preview && npm run pack && electron-packager ./app cgmd-browser --out=builds --platform=darwin --arch=x64 --overwrite=true"
  },
  "author": "nakajmg",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.8.0",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-runtime": "^6.8.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-runtime": "^6.6.1",
    "copy-webpack-plugin": "^3.0.1",
    "cross-env": "^1.0.7",
    "css-loader": "^0.23.1",
    "del": "^2.2.1",
    "devtron": "^1.1.0",
    "electron": "^1.3.1",
    "electron-devtools-installer": "^1.1.4",
    "electron-packager": "^8.0.0",
    "electron-rebuild": "^1.1.3",
    "eslint": "^2.10.2",
    "eslint-config-standard": "^5.1.0",
    "eslint-friendly-formatter": "^2.0.5",
    "eslint-loader": "^1.3.0",
    "eslint-plugin-html": "^1.3.0",
    "eslint-plugin-promise": "^1.0.8",
    "eslint-plugin-standard": "^1.3.2",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "html-webpack-plugin": "^2.16.1",
    "json-loader": "^0.5.4",
    "node-sass": "^3.10.0",
    "sass-loader": "^4.0.2",
    "style-loader": "^0.13.1",
    "tree-kill": "^1.1.0",
    "url-loader": "^0.5.7",
    "vue-hot-reload-api": "^1.3.2",
    "vue-html-loader": "^1.2.2",
    "vue-loader": "^10.0.0",
    "vue-template-compiler": "^2.1.10",
    "vue-style-loader": "^1.0.0",
    "webpack": "^1.13.0",
    "webpack-dev-server": "^1.14.1"
  },
  "dependencies": {
    "axios": "^0.14.0",
    "chokidar": "^1.6.0",
    "codegrid-markdown": "^3.3.0",
    "directory-tree": "^1.1.1",
    "electron-search-text": "^0.3.0",
    "lodash": "^4.16.1",
    "open": "0.0.5",
    "request": "^2.75.0"
  }
}
